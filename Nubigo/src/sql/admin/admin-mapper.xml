<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<entry key="loginMember">
		SELECT *
		FROM MEMBER
		WHERE MEMBER_ID = ?
		  AND MEMBER_PWD = ?
		  AND WITHDRAW_STATUS = 'N'
	</entry>
	
	<entry key="selectAll">
		SELECT * FROM MEMBER
	</entry>

</properties>
